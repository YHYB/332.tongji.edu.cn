<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.scholarship.mapper.ScholarshipMapper">

    <select id="searchByNameYear" resultType="team.scholarship.bean.Scholarship">
        SELECT * FROM scholarship WHERE name = #{name} AND year = #{year}
    </select>

</mapper>
